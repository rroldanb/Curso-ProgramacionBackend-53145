<div class="paginador ">

  <nav aria-label="Page navigation">
    <ul class="pagination pagination-lg">
      {{#if hasPrevPage}}
        <li class="page-item">
          <a
            class="page-link"
            href="{{firstLink}}"
            aria-label="Previous"
          >
            <span aria-hidden="true">&laquo;</span>
          </a>
        </li>
        <li class="page-item">
          <a
            class="page-link"
            href="{{prevLink}}"
            aria-label="Previous"
          >
            <span aria-hidden="true">&lt;</span>
          </a>
        </li>
      {{/if}}
      <li class="page-item bg-dark"><p class="page-link" href="#">
          Mostrando
          {{totalDocs}}
          productos, PÃ¡gina
          {{page}}
          de
          {{totalPages}}</p></li>
      {{#if hasNextPage}}
        <li class="page-item">
          <a
            class="page-link"
            href="{{nextLink}}"
            aria-label="Next"
          >
            <span aria-hidden="true">&gt;</span>
          </a>
        </li>
        <li class="page-item">
          <a
            class="page-link"
            href="{{lastLink}}"
            aria-label="Next"
          >
            <span aria-hidden="true">&raquo;</span>
          </a>
        </li>

      {{/if}}
    </ul>
  </nav>
</div>
